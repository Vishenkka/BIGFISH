<Window x:Class="BIGFISH_БД.DeleteFromTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BIGFISH_БД"
        mc:Ignorable="d"  xmlns:sys="clr-namespace:System;assembly=mscorlib"
           Title="DatePickTotal" Height="200" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">
        <TextBlock Text="Дата начала:"/>
        <DatePicker x:Name="dpStartDate" SelectedDate="{x:Static sys:DateTime.Now}"/>

        <TextBlock Text="Дата конца:" Margin="0,10,0,0"/>
        <DatePicker x:Name="dpEndDate" SelectedDate="{x:Static sys:DateTime.Now}"/>

        <Button Content="Удалить данные" IsDefault="True"  Click="Button_Click"  Margin="0,20,0,0" Background="White"/>
    </StackPanel>
</Window>
